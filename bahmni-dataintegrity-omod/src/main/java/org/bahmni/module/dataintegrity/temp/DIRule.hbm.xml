<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.bahmni.module.dataintegrity.temp.DIRule" table="dataintegrity_rule" schema="openmrs">
        <id name="ruleId">
            <column name="rule_id" sql-type="int(11)"/>
        </id>
        <property name="ruleName">
            <column name="rule_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="ruleCategory">
            <column name="rule_category" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="ruleType">
            <column name="rule_type" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="ruleCode">
            <column name="rule_code" sql-type="varchar(500)" length="500"/>
        </property>

    </class>
</hibernate-mapping>